	#spMainContainer table#spRankInfoSpecial tr.spRankMembersSpecial td:nth-of-type(4):before { content: ""; background: <?php echo($whiteContainer); ?>; border: <?php echo($solidBorder); ?>; color: <?php echo($contentFontColor); ?>; }
