	   border: none;
	}

	#spMainContainer table#spRankInfo tr.spRankData td,
	#spMainContainer table#spRankInfoSpecial tr.spRankDataSpecial td {
		/* Behave  like a "row" */
		border: none;
		border-bottom: <?php echo($solidBorder); ?>;
		position: relative;
		padding-left: 50%;
	}

	#spMainContainer table#spRankInfo tr.spRankData td:before,
	#spMainContainer table#spRankInfoSpecial tr.spRankDataSpecial td:before {
		border: <?php echo($solidBorder); ?>;
		color: <?php echo($contentFontColor); ?>;
		/* Now like a table header */
		position: absolute;
		/* Top/left values mimic padding */
		top: 5px;
		left: 5px;
		width: 45%;
		padding-right: 10px;
		color: <?php echo($contentFontColor); ?>;-space: nowrap;
		text-align: left;
	}

	/* Label the data */
	#spMainContainer table#spRankInfo tr.spRankData td:nth-of-type(1):before { content: attr(data-label); background: <?php echo($whiteContainer); ?>; border: <?php echo($solidBorder); ?>; color: <?php echo($contentFontColor); ?>; }
